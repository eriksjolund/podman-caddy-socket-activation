{
	auto_https disable_redirects
	admin fd/6
}

http://whoami.example.com {
	bind fd/3 {
		protocols h1
	}
	log
	reverse_proxy whoami.example.com:80
}

https://whoami.example.com {
	bind fd/4 {
		protocols h1 h2
	}
	bind fdgram/5 {
		protocols h3
	}
	log
	reverse_proxy whoami.example.com:80
}

http://nginx.example.com {
	bind fd/3 {
		protocols h1
	}
	log
	reverse_proxy nginx.example.com:80
}

https://nginx.example.com {
	bind fd/4 {
		protocols h1 h2
	}
	bind fdgram/5 {
		protocols h3
	}
	log
	reverse_proxy nginx.example.com:80
}
